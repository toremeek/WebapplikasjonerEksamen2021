datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Game {
  id               String  @id @default(cuid())
  combination      String
  user             String?
  numberOfTries    Int?    @default(2)
  foundCombination Boolean @default(false)

}
