datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Game {
  id               String  @id @default(cuid())
  combination      String
  user             String
  numberOfTries    String   @default("0")
  foundCombination String @default("false")

}
